<html>
<body style="">
<div class="content-wrapper" style="min-height:600px;">
    <section class="content">
        <section class="content-header">
            <br>
            <div class="box">
                <div class="box-header with-border">
                    <h4 class="text-left">Users</h4>

                    <div class="box-tools pull-right">

                        <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                                title="Collapse">
                            <i class="fa fa-minus"></i></button>
                        <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip"
                                title="Remove">
                            <i class="fa fa-times"></i></button>
                    </div>
                </div>

                <center>
                    <div class="box-body" style="max-width: 100%">

                        <table id="example" class="table table-striped table-hover" >
                            <thead>
                            <th class="text-center">Sr No.</th>
                            <th  class="text-center">Name</th>
                            <th class="text-center">Email</th>
                            <th  class="text-center">Contact No</th>
                            <th  class="text-center"> Join Date</th>


                            </thead>
                            {{#each cats as |field index  |}}
                                <tr>
                                    <td class="text-center">{{counter index}}</td>
                                    <td class="text-center">{{this.name}}</td>
                                    <td class="text-center">{{this.email}}</td>
                                    <td class="text-center">{{this.mobile}}</td>
                                    <td class="text-center">{{ dateformate this.created_date format dd-mm-yyyyy}}</td>


                                </tr>

                            {{/each}}

                        </table><br>

                    </div>
                </center>
                <div class="box-footer">
                    Footer
                </div>
            </div>
            <!-- /.box -->
        </section>
    </section>
    <!-- /.content -->
</div>

<div class="container">

    <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">New Book Category</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group"><br>
                        <label for="inputEmail3" class="col-sm-2 control-label"> Category</label>

                        <div class="col-sm-10">
                            <input type="email" class="form-control" id="book_cat" id="book_cat" placeholder="Book Category Name">
                        </div>
                        <div id="err_book_cat" class="text-center"></div>
                    </div>

                </div><br>
                <center>   <button type="button" class="btn btn-info"  id="add_cat" style="height: 39px">Add Category</button></center>
                <br><br>
            </div>

        </div>
    </div>

</div>
</body>

</html>